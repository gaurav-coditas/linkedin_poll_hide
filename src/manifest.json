{
    "name": "Linkedin poll remover",
    "version": "2.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://*.linkedin.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "Linkedin poll remover"
    }
}